      },
      {
        "id": "debugger",
        "name": "Debugger & Troubleshooter",
        "workspace": "/Users/YOUR_USER/.openclaw/workspaces/debugger",
        "identity": {
          "name": "Debugger",
          "avatar": "üêõ"
        },
        "groupChat": {
          "mentionPatterns": ["@debugger", "@debug", "@fix"]
        }
      }
    ]
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "hooks": {
    "internal": {
      "enabled": true,
      "entries": {
        "session-memory": {
          "enabled": true
        }
      }
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "REPLACE_WITH_YOUR_TOKEN"
    }
  },
  "skills": {
    "install": {
      "nodeManager": "npm"
    }
  }
}
